{
  "name": "GetImageManifest",
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/repos/[RepoNamespace]/[RepoName]/tags/[Tag]/manifest",
  "parameters": [
    {
      "name": "RepoName",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "RepoNamespace",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "SchemaVersion",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "Tag",
      "position": "Path",
      "type": "String",
      "required": true
    }
  ]
}